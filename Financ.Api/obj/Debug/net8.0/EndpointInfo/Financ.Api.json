  "components": {
    "schemas": {
      "BankInputDTO": {
        "required": [
          "status",
          "titulo"
        ],
        "type": "object",
        "properties": {
          "titulo": {
            "minLength": 1,
            "type": "string"
          },
          "status": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "ProblemDetails": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "detail": {
            "type": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": { }
      },
      "TransectionInputDTO": {
        "required": [
          "dthrReg",
          "status",
          "titulo",
          "valor"
        ],
        "type": "object",
        "properties": {
          "titulo": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "descricao": {
            "maxLength": 800,
            "type": "string",
            "nullable": true
          },
          "valor": {
            "maximum": 99999999,99,
            "minimum": 0,01,
            "type": "number",
            "format": "double"
          },
          "dthrReg": {
            "type": "string",
            "format": "date-time"
          },
          "status": {
            "minLength": 1,
            "type": "string",
            "default": "N"
          }
        },
        "additionalProperties": false
      },
      "TransectionInputDTOBaseTransectionDTO": {
        "required": [
          "bankId"
        ],
        "type": "object",
        "properties": {
          "bankId": {
            "type": "integer",
            "format": "int32"
          },
          "typeTransection": {
            "$ref": "#/components/schemas/TransectionType"
          },
          "content": {
            "$ref": "#/components/schemas/TransectionInputDTO"
          }
        },
        "additionalProperties": false
      },
      "TransectionOutputDTO": {
        "required": [
          "dthrReg",
          "status",
          "titulo",
          "valor"
        ],
        "type": "object",
        "properties": {
          "titulo": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "descricao": {
            "maxLength": 800,
            "type": "string",
            "nullable": true
          },
          "valor": {
            "maximum": 99999999,99,
            "minimum": 0,01,
            "type": "number",
            "format": "double"
          },
          "dthrReg": {
            "type": "string",
            "format": "date-time"
          },
          "status": {
            "minLength": 1,
            "type": "string",
            "default": "N"
          },
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "idFixo": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "TransectionType": {
        "enum": [
          0,
          1,
          2
        ],
        "type": "integer",
        "format": "int32"
      }
    }
  }
}